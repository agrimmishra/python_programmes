{
    "debug.terminal.clearBeforeReusing": false,
    "terminal.integrated.cursorBlinking": true,
    "files.autoSave": "afterDelay",
    "cmake.clearOutputBeforeBuild": false,
    "editor.inlayHints.fontFamily": " \"Arimo\", sans-serif;",
    "editor.experimentalInlineEdit.fontFamily": "\"Arimo\", sans-serif;",
    "editor.fontFamily": "poppins, sans-serif;",
    "editor.codeLensFontFamily": " \"Arimo\", sans-serif;",
    "editor.fontLigatures": true,
    "python.testing.unittestArgs": [
        "-v",
        "-s",
        ".",
        "-p",
        "*test.py"
    ],
    "python.testing.pytestEnabled": false,
    "python.testing.unittestEnabled": true,
    "code-runner.runInTerminal": true,
    "code-runner.saveFileBeforeRun": true,
    "code-runner.saveAllFilesBeforeRun": true,
    "terminal.integrated.cursorStyleInactive": "block",
    "terminal.integrated.enablePersistentSessions": false,
    "terminal.integrated.defaultProfile.windows": "Windows PowerShell",
    "terminal.integrated.fontFamily": "\"aria\",sans-serif",
    "terminal.integrated.fontSize": 12,
    "terminal.integrated.lineHeight": 1.5,
    "terminal.integrated.letterSpacing": 0,
    "terminal.integrated.mouseWheelZoom": true,
    "editor.wordWrap": "on"
}